      subroutine userf  (ix,iy,iz,ieg)
      include 'SIZE'
      include 'TOTAL'
      include 'NEKUSE'
#include "PPICLF.h"

      ie = gllel(ieg)

      ffx = ppiclf_pro_fld(ix,iy,iz,ie,PPICLF_P_JFX)
      ffy = ppiclf_pro_fld(ix,iy,iz,ie,PPICLF_P_JFY)
      
      rdum = 1./vtrans(ix,iy,iz,ie,1)
      rdum = rdum/(1.-ppiclf_pro_fld(ix,iy,iz,ie,PPICLF_P_JPHIP))

      ffx = ffx*rdum
      ffy = ffy*rdum
      ffz = 0.0

      return
      end
